---
import "../styles/general.css";
import Link from "../components/Link.astro";
import MainLayout from "../layouts/MainLayout.astro";
import Accordion from "../components/Accordion.astro";
---

<MainLayout>
  <Link variant="breadcrumb" link="index"
    >Breadcrumb<Link />
    <Link variant="main" link="index"> Main</Link>
    <Accordion question_1="Where do I buy my tickets?" answer_1="You can buy your tickets at Ticketmaster" question_2="Can I charge my phone?" answer_2="Yes, we do have charging station around the festival." />
  </Link>
  <style>
    body {
      background-color: black;
    }
  </style>
</MainLayout>
